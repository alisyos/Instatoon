{
  "version": 2,
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "s-maxage=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ]
} 